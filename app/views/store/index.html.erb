<h2><%= "Signed in as #{current_user.email}" if current_user %></h2>
<% @categories.each do |category| %>
  <%= link_to category.title, category_path(category) %> |
<% end %>
<br></br>
<% @items.each do |item| %>
  <p><%= item.title %> | <%= number_to_currency(item.price.to_f/100) %> <%= button_to 'Add to Cart', line_items_path(item_id: item) %></p><br>
<% end %>